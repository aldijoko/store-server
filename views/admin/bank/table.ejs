<table id="tableNominal" class="table table-bordered table-hover">
    <thead>
    <tr>
      <th>No.</th>
      <th>Nama pemilik</th>
      <th>Nama Bank</th>
      <th>Nomor Rekening</th>
      <th>Action</th>
    </tr>
    </thead>
    <tbody>
      <% for(let i = 0; i < bankData.length; i++) { %>
        <tr>
          <td><%= i + 1 %></td>
          <td><%= bankData[i].name %></td>
          <td><%= bankData[i].bankName %></td>
          <td><%= bankData[i].noRekening %></td>
          <td>
            <form action="/bank/delete/<%= bankData[i]._id %>?_method=DELETE" method="post">
              <a href="/bank/edit/<%= bankData[i]._id %>" class="btn btn-warning btn-sm">
                <i class="fas fa-pencil-alt"></i>
                Edit</a>
              <button class="btn btn-danger btn-sm">
                <i class="fas fa-trash"></i>
                Delete</button>
            </form>
          </td>
        </tr>
      <% } %>
    </tbody>
  </table>